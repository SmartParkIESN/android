<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henallux.projet.smartpark">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:label="Sign in" android:name=".business.SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity  android:label="Sign Up" android:parentActivityName=".business.SignIn" android:name=".business.SignUp"></activity>
        <activity android:label="Main"  android:name=".business.Welcome"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>